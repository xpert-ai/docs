{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "appearance": {
    "default": "light"
  },
  "name": "Xpert AI",
  "colors": {
    "primary": "#2D403C",
    "light": "#3D504C",
    "dark": "#1D302C"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "default": true,
        "products": [
          {
            "product": "AI",
            "description": "AI Module",
            "tabs": [
              {
                "tab": "Overview",
                "groups": [
                  {
                    "group": "Quick Start",
                    "pages": ["en/ai/index", "en/ai/ai-tools/cursor"]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "language": "zh-Hans",
        "products": [
          {
            "product": "AI",
            "description": "AI 平台",    
            "icon": "api",
            "groups": [
              {
                "group": "快速开始",
                "pages": ["zh-Hans/ai/index", "zh-Hans/ai/ai-tools/cursor"]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/zhezhiming/Mintlify"
      },
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Try Chat-Kit",
      "href": "https://xpertai.cn/docs/ai/"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
